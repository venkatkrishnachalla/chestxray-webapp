{
  "name": "cxr-web-app",
  "version": "0.0.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build --prod --aot",
    "test": "ng test --code-coverage --sourceMap=true  --watch=true",
    "lint": "ng lint",
    "generate-mock-data": "node api/generate-mock-data",
    "prestart-mock-api": "npm run generate-mock-data",
    "start-mock-api": "json-server api/db.json -w true -r api/routes.json -m api/middleware.js",
    "pretty-quick": "pretty-quick",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged  && ng lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~9.1.7",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@ngmodule/material-carousel": "^0.6.0",
    "ag-grid-angular": "^23.1.1",
    "ag-grid-community": "^23.1.1",
    "fabric": "^3.6.3",
    "idle-session-timeout": "^1.0.1",
    "html2pdf.js": "^0.9.2",
    "lodash": "^4.17.15",
    "ng5-slider": "^1.2.4",
    "ngx-print": "^1.2.0-beta.5",
    "ngx-toastr": "^13.0.0",
    "perfect-scrollbar": "^1.5.0",
    "rxjs": "~6.5.4",
    "sonar-scanner": "^3.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-schema-faker": "^0.5.0-rcv.24",
    "json-server": "^0.16.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  }
}
