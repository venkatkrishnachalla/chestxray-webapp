{
  "type": "object",
  "required": ["auth"],
  "additionalProperties": false,
  "properties": {
    "auth": {
      "type": "object",
      "required": [
        "idToken",
        "email",
        "refreshToken",
        "expiresIn",
        "localId",
        "registered"
      ],
      "properties": {
        "idToken": {
          "type": "number",
          "multipleOf": 1,
          "unique": true,
          "minimum": 1
        },
        "email": {
          "type": "string",
          "faker": "internet.email"
        },
        "refreshToken": {
          "type": "number",
          "minimum": 0,
          "multipleOf": 1,
          "unique": true
        },
        "expiresIn": {
          "type": "number",
          "const": 30000
        },
        "localId": {
          "type": "string",
          "unique": true,
          "minimum": 1
        },
        "registered": {
          "type": "boolean",
          "const": true
        }
      }
    }
  }
}
